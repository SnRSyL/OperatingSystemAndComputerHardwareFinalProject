#include <stdio.h>

typedef struct {
    int start;
    int size;
} Block;

int main() {
    Block free_list[] = {
        {77,5}, {82,10}, {56,5}, {15,10}, {92,8}
    };

    int count = 5;
    int big_request = 25;
    int success = 0;

    printf("Free list before big allocation:\n");
    for(int i = 0; i < count; i++)
        printf("[start=%d size=%d] ", free_list[i].start, free_list[i].size);

    for(int i = 0; i < count; i++) {
        if (free_list[i].size >= big_request) {
            success = 1;
            break;
        }
    }

    printf("\nBig allocation (%d) ", big_request);
    if (success)
        printf("SUCCESS\n");
    else
        printf("FAIL\n");

    printf("Final free list:\n");
    for(int i = 0; i < count; i++)
        printf("[start=%d size=%d] ", free_list[i].start, free_list[i].size);

    printf("\n");
    return 0;
}

