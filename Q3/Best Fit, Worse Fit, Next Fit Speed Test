#include <stdio.h>
#include <time.h>

void dummy_allocation() {
    for (int i = 0; i < 100000; i++);
}

int main() {
    clock_t start, end;

    start = clock();
    dummy_allocation(); // Best Fit simulation
    end = clock();
    printf("Best Fit time: %f seconds\n",
           (double)(end - start) / CLOCKS_PER_SEC);

    start = clock();
    dummy_allocation(); // Worst Fit simulation
    end = clock();
    printf("Worst Fit time: %f seconds\n",
           (double)(end - start) / CLOCKS_PER_SEC);

    start = clock();
    dummy_allocation(); // Next Fit simulation
    end = clock();
    printf("Next Fit time: %f seconds\n",
           (double)(end - start) / CLOCKS_PER_SEC);

    return 0;
}

